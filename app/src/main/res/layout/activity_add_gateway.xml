<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ardic.android.iotignite.greenhouse.activities.GatewayDashboardActivity"
    android:background="@drawable/farm_background1">



    <include
        android:id="@+id/tool_bar"
        layout="@layout/tool_bar"></include>


    <android.support.design.widget.FloatingActionButton
        android:id="@+id/activity_addgateway_fab_add_a_gateway"
        android:layout_width="78dp"
        android:layout_height="78dp"
        android:clickable="true"
        app:fabSize="normal"
        app:srcCompat="@android:drawable/ic_input_add"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="16dp" />

    <ImageView
        android:id="@+id/activity_addgateway_img_add_a_gateway_text_and_arrow"
        android:layout_width="181dp"
        android:layout_height="151dp"
        app:srcCompat="@drawable/addagateway"
        android:layout_marginEnd="89dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="0.976"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="89dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.421" />

</android.support.constraint.ConstraintLayout>
